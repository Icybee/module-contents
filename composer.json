{
	"name": "icybee/module-contents",
	"type": "icanboogie-module",
	"description": "Base module for content nodes such as articles or news.",
	"keywords": [
		"file",
		"upload"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-contents/issues",
		"source": "https://github.com/Icybee/module-contents"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.5",
		"icanboogie/module-installer": "^1.2",
		"icanboogie/bind-facets": "^0.6",
		"icybee/module-registry": "^3.0",
		"icybee/module-nodes": "^3.0"
	},
	"require-dev": {
		"icybee/core": "^3.0",
		"icybee/module-cache": "^3.0"
	},
	"autoload": {
		"classmap": [
			"lib"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}
